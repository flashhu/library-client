{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@page/*": [
                "src/page/*"
            ],
            "@component/*": [
                "src/component/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@constant/*": [
                "src/constant/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@util/*": [
                "src/util/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}